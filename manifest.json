  "description": "Final fixed version.",
  "permissions": ["activeTab", "scripting", "storage"],
  "host_permissions": ["*://*.zepto.com/*", "*://*.zeptonow.com/*"],
  "action": {
    "default_popup": "popup.html"
  }
}
